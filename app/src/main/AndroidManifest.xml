                android:value=".Cliente.MenuCliente" />
        </activity>
        <activity
            android:name=".Cliente.MenuCliente"
            android:exported="true" android:parentActivityName=".ModuloMaestroCliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ModuloMaestroCliente" />
        </activity>
        <activity
            android:name=".TipoCliente.EditarTipoCliente"
            android:exported="true" />
        <activity
            android:name=".TipoCliente.VerTipoCliente"
            android:exported="true"
            android:parentActivityName=".TipoCliente.MenuTipoCliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TipoCliente.MenuTipoCliente" />
        </activity>
        <activity
            android:name=".TipoCliente.RegistroTipoCliente"
            android:exported="true"
            android:parentActivityName=".TipoCliente.MenuTipoCliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TipoCliente.MenuTipoCliente" />
        </activity>
        <activity
            android:name=".TipoCliente.MenuTipoCliente"
            android:exported="true"
            android:parentActivityName=".ModuloMaestroCliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ModuloMaestroCliente" />
        </activity>
        <activity
            android:name=".EditarZona"
            android:exported="true" />
        <activity
            android:name=".VerZona"
            android:exported="true"
