<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lab02.maestroclientes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaestroClientes">
        <activity
            android:name=".Cliente.EditarCliente"
            android:exported="true" />
        <activity
            android:name=".Cliente.VerCliente"
            android:exported="true"
            android:parentActivityName=".Cliente.MenuCliente">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Cliente.MenuCliente" />
        </activity>
        <activity
            android:name=".Cliente.RegistroCliente"
            android:exported="true" android:parentActivityName=".Cliente.MenuCliente">
